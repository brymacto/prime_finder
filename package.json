{
  "name": "prime-finder",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn install for installing')\"",
    "lint": "yarn run eslint",
    "test": "jest",
    "eslint": "eslint --config ./.eslintrc './**/*.js'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "eslint-config-airbnb": "^17.1.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "yarn": "^1.9.4"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "jest": "^23.5.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/client/"
    ]
  }
}
